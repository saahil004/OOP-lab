#include <iostream>
using namespace std;

int main() {
    float *arr = new float[4];
    for (int i = 0; i < 4; i++) {
        cout << "Enter element " << i << ": ";
        cin >> arr[i];
    }
    float highest = 0;
    float secondHighest = 0;
    for (int i = 0; i < 4; i++) {
        if (arr[i] > highest) {
            highest = arr[i];
        }
    }
    for (int i = 0; i < 4; i++) {
        if (arr[i] > secondHighest && arr[i] != highest) {
            secondHighest = arr[i];
        }
    }
    cout << "The second highest is " << secondHighest << endl;
    delete(arr);
    return 0;
}
